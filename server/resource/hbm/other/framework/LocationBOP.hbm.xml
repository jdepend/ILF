<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.qeweb.framework.bc.sysbop.mobilebop.LocationBOP" table="QEWEB_LOCATION">
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		
		<property name="location" column="location" type="string" length="50"/>
		<property name="address" column="address" type="string" length="200"/>
	</class>
</hibernate-mapping>
